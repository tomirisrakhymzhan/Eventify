res/home/HomePage.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/activities/dashboard/ActivityListItem.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/activities/details/ActivityDetailedSidebar.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/activities/details/ActivityDetailedChat.tsx",["221","222"],"import { Formik, Form, Field, FieldProps } from 'formik'\nimport { observer } from 'mobx-react-lite'\nimport React, { useEffect } from 'react'\nimport { Link } from 'react-router-dom'\nimport { Segment, Header, Comment, Button, Loader } from 'semantic-ui-react'\nimport MyTextArea from '../../../app/common/form/MyTextArea'\nimport { useStore } from '../../../app/stores/store';\nimport * as Yup from 'yup';\nimport { formatDistanceToNow } from 'date-fns'\n\ninterface Props {\n    activityId: string;\n}\n\nexport default observer(function ActivityDetailedChat({ activityId }: Props) {\n    const { commentStore } = useStore();\n\n    useEffect(() => {\n        if (activityId) {\n            commentStore.createHubConnection(activityId);\n        }\n        return () => {\n            commentStore.clearComments();\n        }\n    }, [commentStore, activityId]);\n\n    return (\n        <>\n            <Segment\n                textAlign='center'\n                attached='top'\n                inverted\n                color='teal'\n                style={{ border: 'none' }}\n            >\n                <Header>Chat about this event</Header>\n            </Segment>\n            <Segment attached clearing>\n                <Formik\n                    onSubmit={(values, { resetForm }) =>\n                        commentStore.addComment(values).then(() => resetForm())}\n                    initialValues={{ body: '' }}\n                    validationSchema={Yup.object({\n                        body: Yup.string().required()\n                    })}\n                >\n                    {({ isSubmitting, isValid, handleSubmit }) => (\n                        <Form className='ui form'>\n                            <Field name='body'>\n                                {(props: FieldProps) => (\n                                    <div style={{ position: 'relative' }}>\n                                        <Loader active={isSubmitting} />\n                                        <textarea\n                                            placeholder='Enter your comment (Enter to submit, SHIFT + enter for new line)'\n                                            rows={2}\n                                            {...props.field}\n                                            onKeyPress={e => {\n                                                if (e.key === 'Enter' && e.shiftKey) {\n                                                    return;\n                                                }\n                                                if (e.key === 'Enter' && !e.shiftKey) {\n                                                    e.preventDefault();\n                                                    isValid && handleSubmit();\n                                                }\n                                            }}\n                                        />\n                                    </div>\n                                )}\n                            </Field>\n                        </Form>\n                    )}\n                </Formik>\n                <Comment.Group>\n                    {commentStore.comments.map(comment => (\n                        <Comment key={comment.id}>\n                            <Comment.Avatar src={comment.image || '/assets/user.png'} />\n                            <Comment.Content>\n                                <Comment.Author as={Link} to={`/profiles/${comment.username}`}>\n                                    {comment.displayName}\n                                </Comment.Author>\n                                <Comment.Metadata>\n                                    <div>{formatDistanceToNow(comment.createdAt)} ago</div>\n                                </Comment.Metadata>\n                                <Comment.Text style={{ whiteSpace: 'pre-wrap' }}>{comment.body}</Comment.Text>\n                            </Comment.Content>\n                        </Comment>\n                    ))}\n                </Comment.Group>\n            </Segment>\n        </>\n\n    )\n})","/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/activities/details/ActivityDetailedInfo.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/activities/details/ActivityDetaledHeader.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/activities/dashboard/ActivityFilters.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/errors/TestErrors.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/errors/ValidationErrors.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/errors/NotFound.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/app/stores/commonStore.ts",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/errors/ServerError.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/app/common/form/MyTextArea.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/app/common/form/MyTextInput.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/app/common/options/categoryOptions.ts",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/app/common/form/MySelectInput.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/app/common/form/MyDateInput.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client-app/src/features/users/LoginForm.tsx",[],"/Users/tomirisrahymzan/Dev/C#Projects/Eventify/client